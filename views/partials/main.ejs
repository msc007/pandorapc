<!-- Begin page content -->
<main role="main" class="flex-shrink-0">
  <div class="container">
    <table class="table">
      <thead class="thead-dark">
        <tr>
          <th scope="col">Product Name</th>
          <th scope="col">Vendor</th>
          <th scope="col">Current Price</th>
          <th scope="col">Availability</th>
          <th scope="col">% Sales</th>
          <th scope="col"></th>
        </tr>
      </thead>
      <tbody>
        <% items.forEach( (item) => { item.vendors.forEach( (vendor) => { %>
        <tr>
          <td>
            <a href="<%= vendor.url %>" target="_blank"><%= item.name %> </a>
          </td>
          <td><%= vendor.vendorName %></td>
          <td><%= vendor.currentPrice %></td>
          <td>In stock</td>
          <td>15%</td>
          <td>
            <button
              type="button"
              class="btn btn-dark"
              data-toggle="modal"
              data-target="#subscribeModal"
            >
              Subscribe
            </button>
            <!-- SUBSCRIBE MODAL -->
            <% include subscribeModal %>
          </td>
        </tr>
        <% }) }) %>
      </tbody>
    </table>
  </div>
</main>
